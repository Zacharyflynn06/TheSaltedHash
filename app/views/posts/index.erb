<h1>Here are all the posts</h1>

<div class="post">
    <% @posts.each do |post|%>
        <h3><%= post.title %></h3>
        <h3><%= post.user.username %></h3>
        <h5><%= post.content %></h5>
        <h6>Created on: <%=post.created_at %></h6>
        <hr>
    <% end %>

</div>

<a href="/logout" class="button">Logout</a>   <a href="/posts/new" class="button">New Note</a>